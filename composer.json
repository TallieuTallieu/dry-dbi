{
    "name": "tallieutallieu/dry-dbi",
    "description": "Improved DRY databases",
    "type": "library",
    "require": {
        "php": "^8.1",
        "tallieutallieu/oak": "^3.0.6"
    },
    "autoload": {
        "psr-4": {
            "Tnt\\Dbi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "pestphp/pest": "^3.8"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
